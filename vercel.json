{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/dev-metrics/browser",
  "rewrites": [
    { "source": "/api/cursor/:path*", "destination": "/api/cursor" },
    { "source": "/api/jira/:path*", "destination": "/api/jira" },
    { "source": "/api/bitbucket/:path*", "destination": "/api/bitbucket" },
    { "source": "/((?!api/).*)", "destination": "/index.html" }
  ]
}
